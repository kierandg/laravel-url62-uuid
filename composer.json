{
    "name": "gponster/laravel-url62-uuid",
    "description": "The encoding schema to be used by this shortening algorithm will be Base 62 encoding, as we are going to use 62 letters: [a-zA-Z0-9]. UUID generation based on Rhumsaa\\Uuid",
    "license": "MIT",
    "authors": [
        {
            "name": "Vu Dang a.k.a Gponster",
            "email": "anhvudg@gmail.com"
        }
    ],
    "keywords": ["uuid", "identifier", "guid", "base62", "url62"],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "moontoast/math": "*",
        "rhumsaa/uuid": "~2.8"
    },
    "autoload": {
        "psr-0": {
            "Gponster\\Uuid": "src/"
        }
    },
    "minimum-stability": "stable"
}
